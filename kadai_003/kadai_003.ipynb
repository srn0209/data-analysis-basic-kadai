      "source": [
        "import numpy as np\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "C = np.dot(A, B)\n",
        "print(\"行列の積 C:\\n\", C)\n",
        "print(\"C の最大値:\", np.max(C))\n"
      ]
    }
